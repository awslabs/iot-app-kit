      "group-name": "iot-app-kit",
      "components": [
        "root",
        "components",
        "core",
        "dashboard",
        "react-components",
        "related-table",
        "scene-composer",
        "source-iotsitewise",
        "source-iottwinmaker",
        "testing-util",
        "core-util",
        "jest-config",
        "ts-config",
        "eslint-config"
      ]
    }
  ],
  "pull-request-title-pattern": "chore: release ${version}" ,
  "group-pull-request-title-pattern": "chore: release ${version}"
}
