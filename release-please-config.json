{
  "packages": {
    ".": {
      "component": "root"
    },
    "packages/core": {
      "component": "core"
    },
    "packages/helpers": {
      "component": "helpers"
    },
    "packages/testing-util": {
      "component": "testing-util"
    },
    "packages/core-util": {
      "component": "core-util"
    },
    "packages/dashboard": {
      "component": "dashboard"
    },
    "apps/doc-site": {
      "component": "doc-site"
    },
    "packages/react-components": {
      "component": "react-components"
    },
    "packages/scene-composer": {
      "component": "scene-composer"
    },
    "packages/source-iotsitewise": {
      "component": "source-iotsitewise"
    },
    "packages/source-iottwinmaker": {
      "component": "source-iottwinmaker"
    },
    "packages/tools-iottwinmaker": {
      "component": "tools-iottwinmaker"
    },
    "configuration/ts-config": {
      "component": "ts-config"
    },
    "configuration/eslint-config": {
      "component": "eslint-config"
    }
  },
  "bootstrap-sha": "c37aa7eb3279cf1cbd4653291b7f000f47f7d67c",
  "release-type": "node",
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    },
    {
      "type": "linked-versions",
      "groupName": "iot-app-kit",
      "components": [
        "root",
        "core",
        "helpers",
        "dashboard",
        "doc-site",
        "react-components",
        "scene-composer",
        "source-iotsitewise",
        "source-iottwinmaker",
        "tools-iottwinmaker",
        "testing-util",
        "core-util",
        "ts-config",
        "eslint-config"
      ]
    }
  ],
  "pull-request-title-pattern": "chore: release ${version}",
  "group-pull-request-title-pattern": "chore: release ${version}"
}
