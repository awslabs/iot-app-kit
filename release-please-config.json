{
  "packages": {
    ".": {
      "component": "root"
    },
    "packages/components": {
      "component": "components"
    },
    "packages/core": {
      "component": "core"
    },
    "packages/testing-util": {
      "component": "testing-util"
    },
    "packages/core-util": {
      "component": "core-util"
    },
    "packages/dashboard": {
      "component": "dashboard"
    },
    "packages/react-components": {
      "component": "react-components"
    },
    "packages/related-table": {
      "component": "related-table"
    },
    "packages/scene-composer": {
      "component": "scene-composer"
    },
    "packages/source-iotsitewise": {
      "component": "source-iotsitewise"
    },
    "packages/source-iottwinmaker": {
      "component": "source-iottwinmaker"
    },
    "configuration/ts-config": {
      "component": "ts-config"
    },
    "configuration/jest-config": {
      "component": "jest-config"
    },
    "configuration/eslint-config": {
      "component": "eslint-config"
    }
