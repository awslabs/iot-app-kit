{
  "name": "@iot-app-kit/dashboard",
  "publishConfig": {
    "access": "public"
  },
  "version": "10.10.2",
  "description": "A dashboard widget for IoT App Kit components",
  "homepage": "https://github.com/awslabs/iot-app-kit#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/iot-app-kit.git"
  },
  "bugs": {
    "url": "https://github.com/awslabs/iot-app-kit/issues"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist/",
    "CHANGELOG.md",
    "*NOTICE"
  ],
  "scripts": {
    "compile": "tsc  && tsc-alias && npm run copy-assets",
    "build:es": "tsc -p ./tsconfig.build.es.json && tsc-alias -p ./tsconfig.build.es.json && npm run copy-assets:es",
    "build:cjs": "tsc -p ./tsconfig.build.cjs.json && tsc-alias -p ./tsconfig.build.cjs.json && npm run copy-assets:cjs",
    "copy-assets:es": "copyfiles -e \"**/*.tsx\" -e \"**/*.ts\" -e \"**/*.snap\" -e \"**/*.js\" -e \"**/*.jsx\" -e \"**/*.json\" \"src/**/*\" -u 1 dist/es/",
    "copy-assets:cjs": "copyfiles -e \"**/*.tsx\" -e \"**/*.ts\" -e \"**/*.snap\" -e \"**/*.js\" -e \"**/*.jsx\" -e \"**/*.json\" \"src/**/*\" -u 1 dist/cjs/",
    "build": "npm run clean && npm run build:es && npm run build:cjs",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist",
    "start": "start-storybook -s ./public -p 6006",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "lint": "eslint . --max-warnings=0",
    "fix": "eslint --fix .",
    "test:ui": "npx playwright test",
    "watch-dependencies": "npm exec turbowatch ./turbowatch.ts"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@iot-app-kit/charts-core": "2.1.2",
