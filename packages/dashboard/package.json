{
  "name": "@iot-app-kit/dashboard",
  "publishConfig": {
    "access": "public"
  },
  "version": "10.12.2",
  "description": "A dashboard widget for IoT App Kit components",
  "homepage": "https://github.com/awslabs/iot-app-kit#readme",
  "license": "Apache-2.0",
  "files": [
    "dist/",
    "CHANGELOG.md",
    "*NOTICE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/iot-app-kit.git"
  },
  "bugs": {
    "url": "https://github.com/awslabs/iot-app-kit/issues"
  },
  "types": "dist/types/src/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs && tsc -b ./tsconfig.types.json",
    "build-storybook": "build-storybook",
    "clean": "rimraf dist storybook-static .turbo .cache",
    "clean:nuke": "npm run clean && rimraf node_modules",
    "dev": "npm start",
    "dev:dashboard": "npm run dev",
    "start": "start-storybook -s ./public -p 6006",
    "test": "jest --cacheDirectory .cache/jest",
    "test:watch": "jest --watch --cacheDirectory .cache/jest",
    "lint": "eslint . --max-warnings=0 --cache --cache-location .cache/eslint/",
    "fix": "eslint --fix . --cache --cache-location ./cache/eslint/",
    "test:ui": "npx playwright test",
    "test:ui:ci": "npx playwright test",
    "test:ui:watch": "npx playwright test --ui"
  },
