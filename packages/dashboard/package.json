    "@iot-app-kit/jest-config": "10.10.2",
    "@iot-app-kit/react-components": "10.10.2",
    "@iot-app-kit/source-iotsitewise": "10.10.2",
    "@iot-app-kit/testing-util": "10.10.2",
    "@iot-app-kit/ts-config": "10.10.2",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-styling": "1.3.1",
    "@storybook/builder-webpack5": "^6.5.16",
