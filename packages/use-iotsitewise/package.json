{
  "name": "@iot-app-kit/use-iotsitewise",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "description": "Use the IoT SiteWise service",
  "homepage": "https://github.com/awslabs/iot-app-kit#readme",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": ["dist/", "CHANGELOG.md", "*NOTICE"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/iot-app-kit.git"
  },
  "watch": {
    "build": {
      "patterns": ["src"],
      "extensions": "ts,html,css,tsx",
      "quiet": "false"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:cjs",
    "build:es": "tsc -p ./tsconfig.build.json --outDir ./dist/es",
    "build:cjs": "tsc -p ./tsconfig.build.json --outDir ./dist/cjs --module CommonJS",
    "clean": "rm -rf dist",
    "dev": "npm-watch build",
    "lint": "eslint . --max-warnings=0",
    "fix": "eslint --fix .",
    "test": "npm run test:jest && npm run test:types",
    "test:jest": "TZ=UTC jest --forceExit",
    "test:types": "tsc --noEmit",
    "test.watch": "TZ=UTC jest --watchAll",
    "copy:license": "cp ../../LICENSE LICENSE",
    "copy:notice": "cp ../../NOTICE NOTICE",
    "prepack": "npm run copy:license && npm run copy:notice",
    "pack": "npm pack"
  },
  "dependencies": {
    "@aws-sdk/client-iotsitewise": "3.335.0",
    "@tanstack/react-query": "^4.29.15",
    "react": "latest"
  },
  "devDependencies": {
    "@iot-app-kit/jest-config": "6.3.1",
    "@iot-app-kit/ts-config": "6.3.1",
    "@types/jest": "^29.4.0",
    "eslint-config-iot-app-kit": "6.3.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-extended": "^3.2.4",
    "npm-watch": "^0.11.0",
    "typescript": "^4.9.5"
  },
  "bugs": {
    "url": "https://github.com/awslabs/iot-app-kit/issues"
  }
}
