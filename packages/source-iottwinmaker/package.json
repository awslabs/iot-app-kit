{
  "name": "@iot-app-kit/source-iottwinmaker",
  "publishConfig": {
    "access": "public"
  },
  "version": "10.12.2",
  "description": "AWS IoT TwinMaker source for IoT Application Kit",
  "homepage": "https://github.com/awslabs/iot-app-kit#readme",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist/",
    "CHANGELOG.md",
    "*NOTICE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/iot-app-kit.git"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:cjs",
    "build:es": "tsc -p ./tsconfig.build.json --outDir ./dist/es",
    "build:cjs": "tsc -p ./tsconfig.build.json --outDir ./dist/cjs --module CommonJS",
    "clean": "rimraf dist coverage .tsbuildinfo .turbo",
    "clean:build": "rimraf .tsbuildinfo",
    "dev": "tsc --watch -p ./tsconfig.build.json --outDir ./dist/es",
    "dev:dashboard": "npm run dev",
    "dev:react-components": "npm run dev",
    "lint": "eslint . --max-warnings=0 --cache --cache-location .cache/eslint/",
    "fix": "eslint --fix . --cache --cache-location .cache/eslint/",
    "test": "npm run test:jest",
    "test:jest": "TZ=UTC jest --forceExit --cacheDirectory .cache/jest",
    "test:types": "tsc --noEmit",
    "test:watch": "TZ=UTC jest --watchAll --cacheDirectory .cache/jest",
    "copy:license": "cp ../../LICENSE LICENSE",
    "copy:notice": "cp ../../NOTICE NOTICE",
    "prepack": "npm run copy:license && npm run copy:notice",
    "pack": "npm pack"
  },
  "dependencies": {
    "@aws-sdk/client-iotsitewise": "3.456.0",
    "@aws-sdk/client-iottwinmaker": "3.589.0",
    "@aws-sdk/client-kinesis-video": "3.354.0",
    "@aws-sdk/client-kinesis-video-archived-media": "3.354.0",
    "@aws-sdk/client-s3": "3.335.0",
    "@aws-sdk/client-secrets-manager": "3.353.0",
    "@aws-sdk/url-parser": "3.374.0",
    "@iot-app-kit/core": "*",
    "@tanstack/query-core": "^4.29.15",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.310.0",
    "@iot-app-kit/jest-config": "*",
    "@iot-app-kit/ts-config": "*",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.16.18",
    "aws-sdk-client-mock": "^1.0.0",
    "eslint-config-iot-app-kit": "*",
    "flush-promises": "^1.0.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-extended": "^3.2.4",
    "rimraf": "^5.0.1",
    "typescript": "^4.9.5"
  },
  "bugs": {
    "url": "https://github.com/awslabs/iot-app-kit/issues"
  }
}
