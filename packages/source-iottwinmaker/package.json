{
  "name": "@iot-app-kit/source-iottwinmaker",
  "publishConfig": {
    "access": "public"
  },
  "version": "3.0.0",
  "description": "AWS IoT TwinMaker source for IoT Application Kit",
  "homepage": "https://github.com/awslabs/iot-app-kit#readme",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist/",
    "CHANGELOG.md",
    "*NOTICE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/iot-app-kit.git"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,css,tsx",
      "quiet": "false"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:es",
    "build:cjs": "tsc --outDir dist/cjs --module CommonJS",
    "build:es": "tsc --outDir dist/es",
    "clean": "rm -rf dist",
    "dev": "npm-watch build",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "test": "TZ=UTC jest --forceExit",
    "test.watch": "TZ=UTC jest --watchAll",
    "test:typescript": "tsc --noEmit",
    "copy:license": "cp ../../LICENSE LICENSE",
    "copy:notice": "cp ../../NOTICE NOTICE",
    "prepack": "npm run copy:license && npm run copy:notice",
    "pack": "npm pack"
  },
  "dependencies": {
    "@aws-sdk/client-iotsitewise": "^3.131.0",
    "@aws-sdk/client-iottwinmaker": "^3.131.0",
    "@aws-sdk/client-kinesis-video": "^3.131.0",
    "@aws-sdk/client-kinesis-video-archived-media": "^3.131.0",
    "@aws-sdk/client-s3": "^3.72.0",
    "@aws-sdk/url-parser": "^3.289.0",
    "@iot-app-kit/core": "3.0.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.289.0",
    "@iot-app-kit/jest-config": "3.0.0",
    "@iot-app-kit/ts-config": "3.0.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "aws-sdk-client-mock": "^1.0.0",
    "eslint-config-iot-app-kit": "3.0.0",
    "flush-promises": "^1.0.2",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "npm-watch": "^0.11.0",
    "typescript": "^4.9.5"
  },
  "bugs": {
    "url": "https://github.com/awslabs/iot-app-kit/issues"
  }
}
