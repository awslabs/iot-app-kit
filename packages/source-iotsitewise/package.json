{
  "name": "@iot-app-kit/source-iotsitewise",
  "publishConfig": {
    "access": "public"
  },
  "version": "5.2.0",
  "description": "AWS IoT SiteWise source for IoT Application Kit",
  "homepage": "https://github.com/awslabs/iot-app-kit#readme",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist/",
    "CHANGELOG.md",
    "*NOTICE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/iot-app-kit.git",
    "directory": "packages/source-iotsitewise"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,css,tsx",
      "quiet": "false"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:es && npm run build:cjs",
    "build:es": "tsc -p ./tsconfig.build.json --outDir ./dist/es",
    "build:cjs": "tsc -p ./tsconfig.build.json --outDir ./dist/cjs --module CommonJS",
    "clean": "rm -rf dist",
    "dev": "npm-watch build",
    "lint": "eslint . --max-warnings=0",
    "fix": "eslint --fix .",
    "test": "npm run test:jest && npm run test:types",
    "test:jest": "TZ=UTC jest",
    "test:types": "tsc --noEmit",
    "test.watch": "TZ=UTC jest --watchAll",
    "copy:license": "cp ../../LICENSE LICENSE",
    "copy:notice": "cp ../../NOTICE NOTICE",
    "prepack": "npm run copy:license && npm run copy:notice",
    "pack": "npm pack"
  },
  "dependencies": {
    "@aws-sdk/client-iot-events": "3.281.0",
    "@aws-sdk/client-iotsitewise": "3.281.0",
    "@iot-app-kit/core": "5.2.0",
    "@iot-app-kit/core-util": "5.2.0",
    "@synchro-charts/core": "7.2.0",
    "dataloader": "^2.2.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.uniqwith": "^4.5.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.289.0",
    "@iot-app-kit/jest-config": "5.2.0",
    "@iot-app-kit/testing-util": "5.2.0",
    "@iot-app-kit/ts-config": "5.2.0",
    "@types/jest": "^29.4.0",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.uniqwith": "^4.5.7",
    "eslint-config-iot-app-kit": "5.2.0",
    "flush-promises": "^1.0.2",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "npm-watch": "^0.11.0",
    "typescript": "^4.9.5"
  },
  "bugs": {
    "url": "https://github.com/awslabs/iot-app-kit/issues"
  }
}
