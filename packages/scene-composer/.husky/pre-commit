#!/bin/sh
. "$(dirname "$0")/_/husky.sh"

./node_modules/.bin/formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file translations/IotRociWebGLRenderer.en_US.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format src/utils/totoro-formatter.js && git add translations/IotRociWebGLRenderer.en_US.json
