{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "pack": {
      "dependsOn": [],
      "outputMode": "new-only"
    },
    "lint": {
      "dependsOn": [],
      "outputMode": "new-only"
    },
    "fix": {
      "dependsOn": [],
      "outputMode": "new-only"
    },
    "test": {
      "dependsOn": ["build"],
      "outputMode": "new-only"
    },
    "test:integration": {
      "dependsOn": ["build"],
      "outputMode": "new-only"
    },
    "test:ui": {
      "dependsOn": ["build"],
      "env": ["CI"],
      "outputMode": "new-only"
    },
    "start": {
      "dependsOn": [],
      "env": ["AWS_ACCESS_KEY_ID", "AWS_SECRET_ACCESS_KEY", "AWS_SESSION_TOKEN"],
      "outputMode": "new-only"
    },
    "version": {
      "dependsOn": [],
      "outputMode": "new-only"
    }
  }
}
