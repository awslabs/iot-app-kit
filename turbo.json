{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputLogs": "new-only"
    },
    "@iot-app-kit/dev-env#build": {
      "dependsOn": ["^build"],
      "outputs": [],
      "outputLogs": "new-only"
    },
    "clean": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "//#clean": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "clean:nuke": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "//#clean:nuke": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "dev": {
      "dependsOn": [],
      "persistent": true,
      "cache": false,
      "outputLogs": "full"
    },
    "pack": {
      "dependsOn": ["^clean", "clean", "//#clean", "build"],
      "cache": false,
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [],
      "outputs": [".cache/**"],
      "outputLogs": "new-only"
    },
    "fix": {
      "dependsOn": [],
      "cache": false,
      "outputs": [".cache/**"],
      "outputLogs": "new-only"
    },
    "test": {
      "dependsOn": [],
      "outputs": ["coverage/**", ".cache/**"],
      "outputLogs": "new-only"
    },
    "test:ui": {
      "dependsOn": [],
      "env": ["CI"],
      "outputLogs": "new-only"
    },
    "test:ui:ci": {
      "dependsOn": [],
      "env": ["CI"],
      "outputLogs": "new-only"
    },
    "start": {
      "dependsOn": ["^build"],
      "env": [
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_SESSION_TOKEN",
        "AWS_REGION",
        "ASSET_ID_1",
        "EDGE_GATEWAY_ENDPOINT",
        "NODE_ENV",
        "PROPERTY_ID_1",
        "PROPERTY_ID_2",
        "PROPERTY_ID_3",
        "ALARM_COMPOSITE_MODEL_ID_1",
        "REGION",
        "TZ"
      ]
    },
    "version": {
      "dependsOn": [],
      "outputLogs": "new-only"
    }
  }
}
