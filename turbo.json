{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "styles.css", "loader/**"],
      "outputLogs": "new-only"
    },
    "pack": {
      "dependsOn": [],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["build"],
      "outputLogs": "new-only"
    },
    "fix": {
      "dependsOn": ["build"],
      "outputLogs": "new-only"
    },
    "test": {
      "dependsOn": ["build"],
      "outputLogs": "new-only"
    },
    "test:ui": {
      "dependsOn": ["build"],
      "env": ["CI"],
      "outputLogs": "new-only"
    },
    "test:ui:ci": {
      "dependsOn": ["build"],
      "env": ["CI"],
      "outputLogs": "new-only"
    },
    "start": {
      "dependsOn": ["^build"],
      "env": [
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY",
        "AWS_SESSION_TOKEN",
        "AWS_REGION",
        "ASSET_ID_1",
        "EDGE_GATEWAY_ENDPOINT",
        "NODE_ENV",
        "PROPERTY_ID_1",
        "PROPERTY_ID_2",
        "PROPERTY_ID_3",
        "ALARM_COMPOSITE_MODEL_ID_1",
        "REGION",
        "TZ"
      ]
    },
    "version": {
      "dependsOn": [],
      "outputLogs": "new-only"
    }
  }
}
