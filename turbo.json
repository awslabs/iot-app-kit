{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "outputMode": "new-only"
    },
    "dev": {
      "dependsOn": []
    },
    "pack": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**"],
      "inputs": ["src/**/*.ts", "src/**/*.tsx"],
      "outputMode": "new-only"
    },
    "test:integration": {
      "dependsOn": ["build"],
      "outputMode": "new-only"
    },
    "test:ui": {
      "dependsOn": ["build"],
      "outputMode": "new-only"
    },
    "start": {
      "dependsOn": ["build"]
    },
    "version": {
      "dependsOn": []
    }
  }
}
